# Docker Configuration
# Data directory on host machine for SQLite database
DATA_PATH=./data

# Port to expose the application on
APP_PORT=9876

# Development Configuration
# Backend API port (only used in development)
BACKEND_PORT=3333

# Frontend dev server port (only used in development)
FRONTEND_PORT=5555

# Frontend needs to know backend port (VITE_ prefix required for Vite)
VITE_BACKEND_PORT=3333
